{
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit"
    },
    "editor.formatOnSave": false,
    "vue.updateImportsOnFileMove.enabled": false,
    "vue.codeActions.enabled": false,
    "vue.inlayHints.optionsWrapper": false,
    "editor.wordBasedSuggestions": "off",
    "editor.tabSize": 2,
    "github.copilot.enable": {
      "*": true,
      "plaintext": false,
      "markdown": true,
      "scminput": false
    },
    // "files.watcherExclude": {
      // "**/.git/objects/**": true,
      // "**/node_modules/**": true
    // },
    "files.eol": "\n",
    "files.associations": {
      "*.css": "tailwindcss"
    },
    "editor.quickSuggestions": {
      "strings": "on"
    },
    "files.exclude": {
      "**/.git": true,
      "**/.svn": true,
      "**/.hg": true,
      "**/CVS": true,
      "**/.DS_Store": true,
      "**/Thumbs.db": true,
      "dist": true,
      // "node_modules": true,
      ".output": true,
      // ".nuxt": true
    },
    "search.exclude": {
      "**/node_modules": true,
      "**/bower_components": true,
      "**/*.code-search": true,
      "dist": true,
      "node_modules": true,
      ".output": true,
      ".nuxt": true
    },
    "eslint.useFlatConfig": true,
    "eslint.validate": [
      "javascript",
      "javascriptreact",
      "typescript",
      "typescriptreact",
      "vue",
      "html",
      "markdown",
      "json",
      "jsonc",
      "yaml",
      "toml",
      "xml",
      "gql",
      "tailwindcss",
      "graphql",
      "astro",
      "css",
      "less",
      "scss",
      "pcss",
      "postcss"
    ],
    "typescript.tsdk": "node_modules/typescript/lib",
    "iconify.annotations": false
  }
  